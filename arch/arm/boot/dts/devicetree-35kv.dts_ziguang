		compatible = "gpio-leds";
		#address-cells = <1>;
        #size-cells = <0>;

		qd {
			label = "qd-gpio";
			gpios = <&gpio0 50 0>;
		};
		
		fg {
			label = "fg-gpio";
			gpios = <&gpio0 51 0>;
		};
		
		bhdz {
			label = "bhdz-gpio";
			gpios = <&gpio0 40 0>;
		};
		
