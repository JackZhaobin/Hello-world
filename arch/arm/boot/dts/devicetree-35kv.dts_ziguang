			label = "chzdz-gpio";
			gpios = <&gpio0 41 0>;
		};
		
		yxgj {
			label = "yxgj-gpio";
			gpios = <&gpio0 8 0>;
		};
		
		zzbs {
			label = "zzbs-gpio";
			gpios = <&gpio0 0 0>;
		};
